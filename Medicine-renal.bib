file = {:Users/asshah4/OneDrive - University of Illinois at Chicago/articles/2007{\_}Giannoglou, Chatzizisis, Misirli{\_}The syndrome of rhabdomyolysis Pathophysiology and diagnosis.pdf:pdf},
issn = {09536205},
keywords = {Calcium,Cell death,Diagnosis,Energy depletion,Rhabdomyolysis},
number = {2},
pages = {90--100},
title = {{The syndrome of rhabdomyolysis: Pathophysiology and diagnosis}},
url = {www.elsevier.com/locate/ejim},
volume = {18},
year = {2007}
}
