file = {:Users/asshah4/OneDrive - University of Illinois at Chicago/articles/2014{\_}Berend, de Vries, Gans{\_}Physiological Approach to Assessment of Acid–Base Disturbances.pdf:pdf},
isbn = {1533-4406 (Electronic) 0028-4793 (Linking)},
issn = {0028-4793},
journal = {New England Journal of Medicine},
number = {15},
pages = {1434--1445},
pmid = {25295502},
title = {{Physiological Approach to Assessment of Acid–Base Disturbances}},
